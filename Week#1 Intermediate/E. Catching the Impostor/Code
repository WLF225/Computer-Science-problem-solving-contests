#include <iostream>
#include <set>
 
using namespace std;
 
int main() {
 
    int n, length;
 
    cin >> n >> length;
    set<int> s;
 
    for (int i = 0; i < length; i++) {
        int temp;
        cin >> temp;
        s.insert(temp);
    }
 
    if (s.size() == n-1)
        cout << "YES";
    else
        cout << "NO";
}
