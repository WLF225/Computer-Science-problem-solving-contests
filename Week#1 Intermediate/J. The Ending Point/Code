#include <iostream>

using namespace std;

int main() {
    int x,y;
    cin >> x >> y;

    string s;

    cin >> s;

    for (char c : s) {

        if (c == 'U')
            y++;
        else if (c == 'D')
            y--;
        else if (c == 'R')
            x++;
        else if (c == 'L')
            x--;
    }

    cout << x << " " << y;

}
